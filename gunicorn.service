[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/todo-scratch
ExecStart=/home/ubuntu/archiconda3/envs/env_todo_scratch/bin/gunicorn --config /home/ubuntu/todo-scratch/gunicorn.conf.py 'wsgi:create_app()'
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target